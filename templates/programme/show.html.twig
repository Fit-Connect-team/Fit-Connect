{% extends 'base.html.twig' %}

{% block title %}Programme{% endblock %}

{% block body %}
    <h1>Programme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ programme.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ programme.type }}</td>
            </tr>
            <tr>
                <th>Duree</th>
                <td>{{ programme.duree }}</td>
            </tr>
            <tr>
                <th>Startdate</th>
                <td>        {{ programme.startdate|date('Y-m-d') }}
</td>
            </tr>
            <tr>
                <th>Coach</th>
                <td>{{ programme.coach }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_programme_index') }}">back to list</a>

    <a href="{{ path('app_programme_edit', {'id': programme.id}) }}">edit</a>

    {{ include('programme/_delete_form.html.twig') }}
{% endblock %}
